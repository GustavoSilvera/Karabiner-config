{
  "title": "Change Home/End to CMD+left_arrow/right_arrow",
  "rules": [
    {
      "description": "Change Home/End to command+left_arrow/right_arrow.",
      "manipulators": [
        {
          "from": {
            "key_code": "home",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "left_command"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "end",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "left_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}